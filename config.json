        "dots/.*": "~/",
        "zsh/*": "~/.config/zsh/config.sh",
        "bin/*": "~/.local/bin/",
        "config/*": "~/.config/",
        "launchagents/*.plist":"~/Library/LaunchAgents/",
        "hammerspoon/*": "~/.hammerspoon/"
    },
    "after-scripts": [
        "brew cleanup",
        "find ~/Library/LaunchAgents -name '*.plist' | xargs launchctl load"
    ]
