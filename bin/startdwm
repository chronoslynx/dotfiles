#!/bin/sh
xrdb -merge $HOME/.Xresources
xset +fp /usr/share/fonts/local
xset fp rehash
xscreensaver &
xmodmap $HOME/.Xmodmap
sh $HOME/.screenlayout/default.sh
urxvtd -q -f -o &
feh --bg-scale $HOME/Pictures/ssep.png
eval $(gnome-keyring-daemon --start) &
thunderbird &
pidgin &
start-ipython-notebook &
exec dwmloop
