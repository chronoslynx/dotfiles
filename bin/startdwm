#!/bin/sh
xrdb -merge /home/si-coop123-d01/.Xresources
xset +fp /usr/share/fonts/local
xset fp rehash
xmodmap /home/si-coop123-d01/.Xmodmap
sh /home/si-coop123-d01/.screenlayout/default.sh
urxvtd -q -f -o &
feh --bg-scale /home/si-coop123-d01/Pictures/ssep.png
eval $(gnome-keyring-daemon --start) &
firefox &
thunderbird &
exec dwmloop
