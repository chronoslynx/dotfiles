# Path to your oh-my-zsh configuration.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

ZSH_THEME="gallois"

plugins=(git)

source ~/.fresh/build/shell.sh

if test -d ~/secrets; then
  for i in $HOME/.secrets/*.sh; do
    source $i
  done
fi

